{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=0"
        }
      ]
    },
    {
      "source": "/dashboard",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=0"
        }
      ]
    }
  ]
}
